set number
set ai
set showcmd
set sw=2
set tabstop=2
set expandtab
set hls
set syntax=enable
set showmatch
set whichwrap=""
filetype plugin indent on
map <F12> <C-]>
highlight Constant ctermfg=6*
highlight Comment ctermfg=DarkBlue
highlight DiffAdd ctermfg=LightGreen ctermbg=NONE
highlight DiffDelete ctermfg=LightRed ctermbg=NONE
highlight DiffChange ctermfg=LightYellow ctermbg=NONE
highlight DiffText ctermfg=white ctermbg=DarkYellow
highlight PreProc ctermfg=LightMagenta
highlight Special ctermfg=LightMagenta
highlight ColorColumn ctermbg=DarkGrey
set colorcolumn=+1
